{
  "name": "armor64",
  "version": "0.1.0",
  "type": "module",
  "description": "Safe, strict and stable textual encoding of byte streams.",
  "main": "dist/armor64.js",
  "types": "dist/armor64.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "cli": "node --experimental-transform-types --disable-warning=ExperimentalWarning src/index.ts",
    "dev": "node --experimental-transform-types --env-file=.env --watch src/index.ts",
    "test": "node --experimental-transform-types --test",
    "build": "tsc --declaration"
  },
  "keywords": [
    "encoding",
    "base64",
    "armor64"
  ],
  "author": "YieldRay",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "bin": {
    "armor64": "./dist/cli.js"
  },
  "devDependencies": {
    "@types/node": "^22.18.12",
    "typescript": "^5.9.3"
  }
}
